<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StretchableIndicator">
        <attr name="dotHeight" format="dimension" />
        <attr name="dotWidth" format="dimension" />
        <attr name="frontWidth" format="dimension" />
        <attr name="dotSpace" format="dimension" />
        <attr name="dotFront" format="reference" />
        <attr name="dotBack" format="reference" />
    </declare-styleable>
    <declare-styleable name="TriangleView">
        <attr name="triangle_color" format="color" />
        <attr name="triangle_direction" format="enum">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>
</resources>